{
	"name": "base-admin",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"description": "base-admin open source management system",
	"author": {
		"name": "Hickey",
		"email": "h_base@163.com",
		"url": "https://github.com/h-base"
	},
	"license": "MIT",
	"homepage": "https://github.com/h-base/Bae-Admin",
	"scripts": {
		"dev": "vue-tsc && vite",
		"build:test": "vue-tsc && vite build --mode test",
		"build:pro": "vue-tsc && vite build --mode production",
		"preview": "vite preview",
		"eslint": "eslint --ext .ts,.vue --ignore-path .gitignore --fix .",
		"prettier": "prettier -c .prettierrc.cjs -w .",
		"commit": "git cz",
		"lint-staged": "lint-staged",
		"prepare": "husky install"
	},
	"dependencies": {
		"@commitlint/cli": "^17.6.6",
		"@commitlint/config-conventional": "^17.6.6",
		"@element-plus/icons-vue": "^2.1.0",
		"@vueuse/core": "^10.1.2",
		"@wangeditor/editor": "^5.1.23",
		"@wangeditor/editor-for-vue": "^5.1.12",
		"axios": "^1.4.0",
		"dayjs": "^1.11.7",
		"echarts": "^5.4.2",
		"echarts-liquidfill": "^3.1.0",
		"element-plus": "^2.3.4",
		"init": "^0.1.2",
		"lint-staged": "^13.2.3",
		"mitt": "^3.0.0",
		"nprogress": "^0.2.0",
		"pinia": "^2.1.3",
		"pinia-plugin-persistedstate": "^3.1.0",
		"qs": "^6.11.2",
		"sortablejs": "^1.15.0",
		"vue": "^3.3.4",
		"vue-router": "^4.2.1",
		"vuedraggable": "^4.1.0"
	},
	"devDependencies": {
		"@types/nprogress": "^0.2.0",
		"@types/qs": "^6.9.7",
		"@types/sortablejs": "^1.15.1",
		"@typescript-eslint/eslint-plugin": "^5.55.0",
		"@typescript-eslint/parser": "^5.55.0",
		"@vitejs/plugin-vue": "^4.2.3",
		"@vitejs/plugin-vue-jsx": "^3.0.1",
		"autoprefixer": "^10.4.14",
		"commitizen": "^4.3.0",
		"commitlint-config-cz": "^0.13.3",
		"cz-conventional-changelog": "^3.3.0",
		"cz-customizable": "^7.0.0",
		"eslint": "^8.36.0",
		"eslint-config-prettier": "^8.7.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-vue": "^9.9.0",
		"husky": "^8.0.3",
		"prettier": "^2.8.4",
		"rollup-plugin-visualizer": "^5.9.0",
		"sass": "^1.62.1",
		"typescript": "^5.0.4",
		"unplugin-vue-setup-extend-plus": "^1.0.0",
		"vite": "^4.3.8",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-eslint": "^1.8.1",
		"vite-plugin-html": "^3.2.0",
		"vite-plugin-pwa": "^0.15.0",
		"vite-plugin-svg-icons": "^2.0.1",
		"vue-tsc": "^1.6.5"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-customizable"
		},
		"cz-customizable": {
			"config": ".cz-config.cjs"
		}
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -e $GIT_PARAMS"
		}
	},
	"lint-staged": {
		"*.{js,ts,vue,jsx,tsx}": [
			"eslint --cache --fix",
			"git add ."
		]
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"browserslist": {
		"production": [
			"> 1%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
